[logout]
full_text=" <span font='FontAwesome'>\uf011</span> "
markup=pango
command=xfce4-session-logout

[calendar]
full_text=" <span font='FontAwesome'>\uf073</span> "
markup=pango
command=gsimplecal

[time]
command=echo '' $(date '+%Y-%m-%d %H:%M') ''
interval=1
